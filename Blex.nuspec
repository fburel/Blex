    <file src="Blex.Touch\bin\Release\net8.0-ios\Blex.Touch.dll" target="lib\net8.0-ios17.0" />

    <!-- Android reference assemblies -->
    <file src="Blex.Droid\bin\Release\net8.0-android\Blex.dll" target="lib\net8.0-android26.0" />
    <file src="Blex.Droid\bin\Release\net8.0-android\Blex.Droid.dll" target="lib\net8.0-android26.0" />
