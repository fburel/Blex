<?xml version="1.0"?>
<package >
  <metadata>
    <id>Blex.f10-solutions</id>
    <version>1.0.3</version>
    <title>Blex</title>
    <authors>f10-solutions</authors>
    <owners>f10-solutions</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Blex is a cross platform BLE library for Xamarin.</description>
    <summary>Blex brings the native Bluetooth Low Energy (a.k.a Bluetooth 4.0) capacities of Android and iOS into the Xamarin cross platform world.</summary>
    <releaseNotes>Add logs and log forwarding capabilities</releaseNotes>
    <copyright>Copyright 2020</copyright>
    <tags>bluetooth low energy ble iot</tags>
    <projectUrl>https://github.com/fburel/Blex</projectUrl>
    <repository type="git" url="https://github.com/fburel/Blex.git" branch="master" commit="7ef6daec295d60a32d330b61c8440b668fa8d64d" />
    <license type="expression">MIT</license>

    <dependencies>
        <group targetFramework=".NETStandard2.0">
            <!--MonoAndroid dependencies go here-->
        </group>
        <group targetFramework="MonoAndroid">
            <!--MonoAndroid dependencies go here-->
        </group>
        <group targetFramework="Xamarin.iOS10">
            <!--Xamarin.iOS10 dependencies go here-->
        </group>
    </dependencies>
  </metadata>
  <files>
    <!-- Cross-platform reference assemblies -->
    <file src="Blex\bin\Release\netstandard2.0\Blex.dll" target="lib\netstandard2.0\Blex.dll" />

    <!-- iOS reference assemblies -->
    <file src="Blex\bin\Release\netstandard2.0\Blex.dll" target="lib\Xamarin.iOS10\Blex.dll" />
    <file src="Blex.Touch\bin\Release\Blex.Touch.dll" target="lib\Xamarin.iOS10\Blex.Touch.dll" />

    <!-- Android reference assemblies -->
    <file src="Blex\bin\Release\netstandard2.0\Blex.dll" target="lib\MonoAndroid10\Blex.dll" />
    <file src="Blex.Droid\bin\Release\Blex.Droid.dll" target="lib\MonoAndroid10\Blex.Droid.dll" />
  </files>
</package>